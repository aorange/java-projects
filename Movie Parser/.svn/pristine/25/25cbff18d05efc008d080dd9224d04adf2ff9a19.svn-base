import java.util.ArrayList;
/**
 * List of Movies class since we have more than one movie in the JSON.
 * @author Allen Tang
 * @version 1.0
 */
public class MoviesList {
    private ArrayList <Movies> results;

    public ArrayList <Movies> getresults() {
        return results;
    }

    public void appendresults(ArrayList <Movies> additions) {
        for (Movies x : additions){
            results.add(x);
        }
    }
}
